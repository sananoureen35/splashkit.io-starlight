   "source": [
    "def is_number(s):\n",
    "    try:\n",
    "        float(s)\n",
    "        return True\n",
    "    except ValueError:\n",
    "        return False\n",
    "\n",
    "input_str = input(\"Enter the value: \")\n",
    "\n",
    "if is_number(input_str):\n",
    "    print(f\"{input_str} is a number.\")\n",
    "else:\n",
    "    print(f\"{input_str} is not a number.\")"
   ]
  }
